{
    "authors": [
        "Richard (Rikki) Andrew Cattermole"
    ],
    "copyright": "Copyright Â© 2022, Richard Cattermole",
    "description": "Basic memory handling for -betterC and beyond.",
    "license": "Artistic-2.0",
    "name": "sidero_base",
    "homepage": "https://github.com/Project-Sidero/basic_memory",
    "importPaths": [],
    "sourcePaths": [],
    "sourceFiles": [
        "source/ignoreme.d"
    ],
    "injectSourceFiles": [
        "source/sidero/base/allocators/gc_hook.d"
    ],
    "dependencies": {
        "sidero_base:source": "*"
    },
    "subPackages": [
        {
            "name": "verify_generated",
            "sourcePaths": [
                "generated",
                "tools/verify_generated",
                "source"
            ],
            "targetType": "executable",
            "dflags-posix": [
                "-allinst"
            ],
            "lflags-windows": [
                "/IGNORE:4217",
                "/FORCE:MULTIPLE"
            ],
            "libs-windows": [
                "Ws2_32",
                "advapi32",
                "Userenv",
                "Netapi32"
            ],
            "versions": [
                "Windows7"
            ]
        },
        {
            "name": "rungenerators",
            "targetType": "staticLibrary",
            "sourcePaths": [],
            "sourceFiles": [
                "source/ignoreme.d"
            ],
            "importPaths": [],
            "extraDependencyFiles": [
                "tools/templates/sidero/base/text/unicode/readonly.d",
                "tools/templates/sidero/base/text/unicode/builder.d"
            ],
            "preBuildCommands-posix": [
                "DFLAGS=\"\" ${DUB_EXE} run --bare --skip-registry=all --root=${SIDERO_BASE:RUNGENERATORS_PACKAGE_DIR}/tools :preBuild"
            ],
            "preBuildCommands-windows": [
                "set DFLAGS= && ${DUB_EXE} run --bare --skip-registry=all --root=${SIDERO_BASE:RUNGENERATORS_PACKAGE_DIR}/tools :preBuild"
            ]
        },
        {
            "name": "source",
            "targetType": "sourceLibrary",
            "importPaths": [
                "source",
                "generated"
            ],
            "sourcePaths": [
                "source",
                "generated"
            ],
            "excludedSourceFiles": [
                "source/sidero/base/text/unicode/readonly.d",
                "source/sidero/base/text/unicode/builder.d"
            ],
            "sourceFiles-windows-dmd": [
                "msvc_exports.def"
            ],
            "dflags-windows-ldc": [
                "--fvisibility=public"
            ],
            "dflags-ldc": [
                "--linkonce-templates",
                "--link-internally"
            ],
            "dflags": [
                "-preview=dip1000"
            ],
            "dflags-posix": [
                "-allinst"
            ],
            "lflags-windows": [
                "/IGNORE:4217",
                "/FORCE:MULTIPLE"
            ],
            "libs-windows": [
                "Ws2_32",
                "advapi32",
                "Userenv",
                "Netapi32",
                "mincore"
            ],
            "versions": [
                "Windows7"
            ]
        }
    ],
    "configurations": [
        {
            "name": "library",
            "targetType": "dynamicLibrary",
            "versions": [
                "DynamicSideroBase"
            ],
            "buildOptions": [
                "betterC"
            ]
        },
        {
            "name": "static",
            "targetType": "staticLibrary",
            "buildOptions": [
                "betterC"
            ]
        },
        {
            "name": "unittest",
            "versions": [
                "InitAfterDruntimeSideroBase"
            ]
        },
        {
            "name": "executable",
            "targetType": "executable"
        }
    ]
}
